        Collection&lt;Student&gt; l = <span class="keyword-directive">new</span> TreeSet&lt;Student&gt;();
        <span class="keyword-directive">for</span> (Student si : students) {
            <span class="keyword-directive">if</span> (si.getGroupId() == group.getGroupId() &amp;&amp; si.getEducationYear() == year) {
