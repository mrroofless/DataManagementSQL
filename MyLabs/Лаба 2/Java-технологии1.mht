        <span class="keyword-directive">for</span> (Student si : students) {
            <span class="keyword-directive">if</span> (si.getGroupId() == oldGroup.getGroupId() &amp;&amp; si.getEducationYear() == oldYear) {
                si.setGroupId(newGroup.getGroupId());
                si.setEducationYear(newYear);
