<span class="keyword-directive">import</span> java.util.Locale;

<span class="keyword-directive">public</span> <span class="keyword-directive">class</span> Test
{
  <span class="keyword-directive">public</span> <span class="keyword-directive">static</span> <span class="keyword-directive">void</span> main(String[] arg) {

    Collator c = Collator.getInstance(<span class="keyword-directive">new</span> Locale(<span class="character">&quot;</span><span class="character">ru</span><span class="character">&quot;</span>));
    <span class="comment">// Если закомментировать следующую строку, то &quot;ПАВЕЛ&quot; будет не равен &quot;Павел&quot; </span>
    <span class="comment">// (c.compare!=0) что по идее не совсем корректно</span>
    c.setStrength(Collator.PRIMARY);
    System.out.println(c.compare(<span class="character">&quot;</span><span class="character">ПАВЕЛ</span><span class="character">&quot;</span>, <span class="character">&quot;</span><span class="character">ПАВЕЛ</span><span class="character">&quot;</span>));
    System.out.println(c.compare(<span class="character">&quot;</span><span class="character">ПАВЕЛ</span><span class="character">&quot;</span>, <span class="character">&quot;</span><span class="character">Павел</span><span class="character">&quot;</span>));
    System.out.println(c.compare(<span class="character">&quot;</span><span class="character">ПАВЕЛ</span><span class="character">&quot;</span>, <span class="character">&quot;</span><span class="character">артем</span><span class="character">&quot;</span>));
    System.out.println(<span class="character">&quot;</span><span class="character">ПАВЕЛ</span><span class="character">&quot;</span>.compareTo(<span class="character">&quot;</span><span class="character">артем</span><span class="character">&quot;</span>));
  }
}
