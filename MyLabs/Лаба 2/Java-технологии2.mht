        } <span class="keyword-directive">catch</span> (SQLException e) {
            <span class="keyword-directive">throw</span> <span class="keyword-directive">new</span> Exception(e);
        }
    }

    <span class="keyword-directive">public</span> <span class="keyword-directive">static</span> <span class="keyword-directive">synchronized</span> ManagementSystem getInstance() <span class="keyword-directive">throws</span> Exception {
        <span class="keyword-directive">if</span> (instance == <span class="keyword-directive">null</span>) {
